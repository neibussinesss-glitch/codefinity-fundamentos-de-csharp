using System;

namespace ConsoleApp {
    public class Program {
        public static void Main(string[] args) {
            string name = "Alice";
            int age = 20;

            string all = $"Hello, my name is {name} and I am {age} years old.";

            Console.WriteLine(all);
        }

        public static void TestMain(string[] args) { Main(args); }
    }
}